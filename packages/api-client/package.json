{
  "name": "@worklog/api-client",
  "version": "1.0.0",
  "description": "WorkLog API Client generated from OpenAPI specification",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "generate": "openapi-generator-cli generate -i http://localhost:3001/api-docs.json -g typescript-axios -o ./src --additional-properties=typescriptThreePlus=true,useSingleRequestParameter=true",
    "build": "tsc",
    "prepublishOnly": "npm run generate && npm run build"
  },
  "dependencies": {
    "axios": "^1.6.7"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "typescript": "^5.3.3",
    "@types/node": "^20.11.24"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "api-client",
    "typescript",
    "openapi",
    "worklog"
  ],
  "author": "WorkLog Team",
  "license": "MIT"
}